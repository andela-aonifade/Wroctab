{
  "name": "wroctabserver",
  "version": "1.0.0",
  "description": "Wroctab is a document management system application that allow users to view document based on their access level",
  "main": "server.js",
  "scripts": {
    "seed:test": "",
    "reset:test": "",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "build": "rm -rf dist/ && babel ./ --copy-files -d dist --presets es2015,stage-2  --ignore ./node_modules",
    "start": "npm-run-all build start:nodemon",
    "db:migrate": "sequelize db:migrate",
    "start:nodemon": "nodemon server.js --exec babel-node --presets es2015,stage-2",
    "test:server": "NODE_ENV=test babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/.bin/_mocha server/tests/**/*.spec.js",
    "test": "mocha --reporter progress test/client/testSetup.js \"test/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com:andela-aonifade/Wroctab.git"
  },
  "author": "Anuoluwapo Onifade",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.1",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "faker": "^4.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-webpack": "^1.5.0",
    "inline-style-prefixer": "^3.0.3",
    "jsdom": "^10.1.0",
    "jsonwebtoken": "^7.4.0",
    "jwt-decode": "^2.2.0",
    "material-ui": "^0.18.0",
    "materialize-css": "^0.98.2",
    "morgan": "^1.8.1",
    "npm-run-all": "^4.0.2",
    "path": "^0.12.7",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "devDependencies": {
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.1.0",
    "gulp-nodemon": "^2.2.1",
    "jquery": "^3.2.1",
    "mocha": "^3.3.0",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^6.0.4",
    "webpack-dev-server": "^2.4.5"
  }
}
